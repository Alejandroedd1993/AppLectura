{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": ["warn", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }],
    "no-undef": "warn",
    "no-empty": ["warn", { "allowEmptyCatch": true }],
    "no-case-declarations": "warn",
    "no-prototype-builtins": "warn",
    "no-useless-escape": "warn",
    "no-console": ["off"]
  }
}
